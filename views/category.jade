extends layout

block css
	link(rel='stylesheet',href='/stylesheets/style.css')
	link(rel='stylesheet',href='/stylesheets/fonts.css')
	link(rel='stylesheet',href='/stylesheets/prettify.css')

block content
	article
		h2 Results for #{category}
		each val in list
			a(href='/article/'+val.url)= val.title
			p= val.previewtext
				a.readmore(href='/article/'+val.url) [&nbsp;Read More...&nbsp;]
